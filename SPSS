  ELEMENT: interval(position(INDEX*SUMMARY*adulto_mayor), color.interior(INDEX), 
    shape.interior(shape.square))
END GPL.

CROSSTABS
  /TABLES=acceso_ordinal BY qaut
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT COLUMN 
  /COUNT ROUND CELL.

CROSSTABS
  /TABLES=acceso_ordinal BY migrante
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT COLUMN 
  /COUNT ROUND CELL.

CROSSTABS
  /TABLES=acceso_ordinal BY migrante indigena adulto_mayor
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT COLUMN 
  /COUNT ROUND CELL.

CROSSTABS
  /TABLES=acceso_ordinal BY area
  /FORMAT=AVALUE TABLES
  /CELLS=COUNT COLUMN 
  /COUNT ROUND CELL.


COMPUTE indice_acceso=acceso_total / 3.
EXECUTE.

FREQUENCIES VARIABLES=indice_acceso
  /STATISTICS=STDDEV MINIMUM MAXIMUM MEAN MEDIAN
  /ORDER=ANALYSIS.


USE ALL.
COMPUTE filter_$=(edad >= 60).
VARIABLE LABELS filter_$ 'edad >= 60 (FILTER)'.
VALUE LABELS filter_$ 0 'Not Selected' 1 'Selected'.
FORMATS filter_$ (f1.0).
FILTER BY filter_$.
EXECUTE.


FREQUENCIES VARIABLES=indice_acceso
  /STATISTICS=STDDEV MINIMUM MAXIMUM MEAN MEDIAN
  /BARCHART PERCENT
  /ORDER=ANALYSIS.

FILTER OFF.
USE ALL.
EXECUTE.

USE ALL.
COMPUTE filter_$=(migrante = 1).
VARIABLE LABELS filter_$ 'migrante = 1 (FILTER)'.
VALUE LABELS filter_$ 0 'Not Selected' 1 'Selected'.
FORMATS filter_$ (f1.0).
FILTER BY filter_$.
EXECUTE.

FREQUENCIES VARIABLES=indice_acceso
  /STATISTICS=STDDEV MINIMUM MAXIMUM MEAN MEDIAN
  /BARCHART PERCENT
  /ORDER=ANALYSIS.

FILTER OFF.
USE ALL.
EXECUTE.

USE ALL.
COMPUTE filter_$=(indigena = 1).
VARIABLE LABELS filter_$ 'indigena = 1 (FILTER)'.
VALUE LABELS filter_$ 0 'Not Selected' 1 'Selected'.
FORMATS filter_$ (f1.0).
FILTER BY filter_$.
EXECUTE.

FREQUENCIES VARIABLES=indice_acceso
  /STATISTICS=STDDEV MINIMUM MAXIMUM MEAN MEDIAN
  /BARCHART PERCENT
  /ORDER=ANALYSIS.

AGGREGATE
  /OUTFILE='C:\Users\CelisJR9\Desktop\analisis electivo\agregado_adulto_mayor.sav'
  /BREAK=adulto_mayor
  /indice_acceso_mean_1=MEAN(indice_acceso) 
  /indice_acceso_sd=SD(indice_acceso) 
  /indice_acceso_n=N(indice_acceso).

AGGREGATE
  /OUTFILE='C:\Users\CelisJR9\Desktop\analisis electivo\agregado_migrante.sav'
  /BREAK=migrante
  /indice_acceso_mean_1=MEAN(indice_acceso) 
  /indice_acceso_sd=SD(indice_acceso) 
  /indice_acceso_n=N(indice_acceso).

AGGREGATE
  /OUTFILE='C:\Users\CelisJR9\Desktop\analisis electivo\agregado_indigena.sav'
  /BREAK=indigena
  /indice_acceso_mean_1=MEAN(indice_acceso) 
  /indice_acceso_sd=SD(indice_acceso) 
  /indice_acceso_n=N(indice_acceso).



T-TEST GROUPS=adulto_mayor(0 1)
  /MISSING=ANALYSIS
  /VARIABLES=indice_acceso
  /CRITERIA=CI(.95).

T-TEST GROUPS=migrante(0 1)
  /MISSING=ANALYSIS
  /VARIABLES=indice_acceso
  /CRITERIA=CI(.95).

T-TEST GROUPS=indigena(0 1)
  /MISSING=ANALYSIS
  /VARIABLES=indice_acceso
  /CRITERIA=CI(.95).


